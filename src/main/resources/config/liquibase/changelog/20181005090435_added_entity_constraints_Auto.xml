<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Auto.
    -->
    <changeSet id="20181005090435-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cds_id"
                                 baseTableName="auto"
                                 constraintName="fk_auto_cds_id"
                                 referencedColumnNames="id"
                                 referencedTableName="istituti"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="auto"
                                 constraintName="fk_auto_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
